# --- DOMAIN NAMES ---
SEARXNG_BASE_URL=search.example.com
OPENWEBUI_HOSTNAME=ai.example.com
CLIPROXY_HOSTNAME=proxy.example.com

# --- DOCKER IMAGES ---
# Override default images if needed
CADDY_IMAGE=caddy:2.8.4-alpine
OPENWEBUI_IMAGE=ghcr.io/open-webui/open-webui:main
OLLAMA_IMAGE=ollama/ollama:latest
CLI_PROXY_IMAGE=ghcr.io/ai-dock/comfyui:latest

# --- SEARXNG CONFIGURATION ---
SEARXNG_PORT=8080
SEARXNG_BIND_ADDRESS=0.0.0.0
SEARXNG_METHOD=GET
SEARXNG_IMAGE_PROXY=false
SEARXNG_DEBUG=false
SEARXNG_VALKEY_URL=redis://valkey:6379/0

# --- SECURITY ---
# Generate these once and keep them secret!
# Run: openssl rand -hex 32
WEBUI_SECRET_KEY=
SEARXNG_SECRET=

# --- OPTIONAL ---
LETSENCRYPT_EMAIL=internal
SEARXNG_PUBLIC_INSTANCE=false
SEARXNG_LIMITER=false

# --- DEPLOYMENT ---
# Set to 'production' or 'development'
DEPLOY=production

# --- CORS SETTINGS (Production) ---
# Set to 'true' and specify your domain to avoid '*' security warning
ENABLE_CORS=false
CORS_ALLOW_ORIGIN=https://ai.example.com

# --- BACKUP SETTINGS ---
# Directory where backups will be stored (relative or absolute)
BACKUP_DIR=./backups
# How many days to keep backups
BACKUP_RETENTION=7
